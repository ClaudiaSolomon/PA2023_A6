<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">
        <class>org.example.Lab9.Entity.AlbumsEntity</class>
        <class>org.example.Lab9.Entity.ArtistsEntity</class>
        <class>org.example.Lab9.Entity.GenresEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/java"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user" value="claudia"/>
            <property name="eclipselink.jdbc.password" value="claudia"/>
        </properties>
    </persistence-unit>
</persistence>
